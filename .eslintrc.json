{
  "extends": ["@opencast/eslint-config-ts-react"],
  "plugins": ["@stylistic/eslint-plugin-ts"],
  "rules": {
    "@stylistic/ts/member-delimiter-style": "warn"
  },
  "overrides": [
      {
          "files": ["./webpack.config.ts"],
          "parserOptions": {
              "project": false
          }
      }
  ],
  "ignorePatterns": [
    "node_modules",
    "old-src",
    "build"
  ]
}
